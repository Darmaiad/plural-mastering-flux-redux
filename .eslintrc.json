{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "google",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
		"max-len": [
			"error", 
			100
		],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
		"arrow-parens": [
            "error",
            "as-needed"
        ],
		"no-tabs": 0,
		"object-curly-spacing": 0,
		"require-jsdoc": 0
			
		
    }
}
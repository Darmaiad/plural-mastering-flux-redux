{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
       "google",
       "plugin:react/recommended"
        // "eslint:recommended",
        // "react-app"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "eol-last": [
            "error",
            "always"
        ],
        "max-len": [
            "error",
            120
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "arrow-parens": [
            "error",
            "as-needed"
        ],
        "no-tabs": 0,
        "object-curly-spacing": 0,
        "require-jsdoc": 0
    }
}